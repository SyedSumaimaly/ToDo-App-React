 index) => {\n          return (\n            <Box className=\"eachitems\" key={index}>\n              <div className=\"eachitems_sub\">\n                <p className={value.status && \"checked\"}>{value.items}</p>\n              </div>\n              <Box className=\"icons\">\n                <FontAwesomeIcon\n                  className=\"Deleteicon\"\n                  onClick={() => {\n                    deleteitem(index);\n                  }}\n                  icon={faTrash}\n                  beatFade\n                />\n\n                <FontAwesomeIcon\n                  shake\n                  data-status=\"false\"\n                  className=\"Done hello\"\n                  onClick={() => {\n                    handleDone(value, index);\n                  }}\n                  icon={faCircleCheck}\n                />\n\n                <FontAwesomeIcon\n                  className={value.status ? \"Edit disabled\" : \"Edit\"}\n                  onClick={() => {\n                    handleEdit(value, index);\n                  }}\n                  shake\n                  icon={faPenToSquare}\n                />\n              </Box>\n            </Box>\n          );\n        })}\n        <Button onClick={deleteall} variant=\"outlined\" color=\"error\">\n          Delete All\n        </Button>\n      </div>\n    </>\n  );\n}\n\nexport default App;\n",{"ruleId":"30","severity":1,"message":"31","line":1,"column":17,"nodeType":"32","messageId":"33","endLine":1,"endColumn":26},{"ruleId":"30","severity":1,"message":"34","line":3,"column":8,"nodeType":"32","messageId":"33","endLine":3,"endColumn":17},{"ruleId":"30","severity":1,"message":"35","line":16,"column":10,"nodeType":"32","messageId":"33","endLine":16,"endColumn":17},{"ruleId":"30","severity":1,"message":"36","line":17,"column":10,"nodeType":"32","messageId":"33","endLine":17,"endColumn":14},{"ruleId":"30","severity":1,"message":"37","line":23,"column":10,"nodeType":"32","messageId":"33","endLine